{
  "name": "dp-sanity-cms",
  "private": true,
  "version": "1.0.0",
  "description": "CMS/sanity-studio for dagpengerelatert innhold",
  "main": "package.json",
  "author": "NAV",
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "npm run deploySchema && npm run deployProdCloneSchema && npm run deployDevSchema && npm run deployDemoSchema && npm run deployStudio",
    "deployStudio": "sanity deploy",
    "deploySchema": "sanity graphql deploy --no-playground",
    "deployProdCloneSchema": "sanity graphql deploy --no-playground --dataset prodclone",
    "deployDevSchema": "sanity graphql deploy --no-playground --dataset development",
    "deployDemoSchema": "sanity graphql deploy --no-playground --dataset demo",
    "backup": "curl https://rt6o382n.api.sanity.io/v1/data/export/production/ > backup.ndjson",
    "clearDevelopmentDataset": "sanity dataset delete development && sanity dataset create development --visibility private",
    "clearProdCloneDataset": "sanity dataset delete prodclone && sanity dataset create prodclone --visibility private",
    "copyProdDataToDevelopment": "npm run backup && npm run clearDevelopmentDataset && sanity dataset import backup.ndjson development --replace",
    "copyProdDataToProdClone": "npm run backup && npm run clearProdCloneDataset && sanity dataset import backup.ndjson prodclone --replace"
  },
  "dependencies": {
    "@sanity/base": "^1.149.16",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^2.0.9",
    "@sanity/components": "^1.149.16",
    "@sanity/core": "^1.149.17",
    "@sanity/dashboard": "^1.149.16",
    "@sanity/default-layout": "^1.149.16",
    "@sanity/default-login": "^1.149.16",
    "@sanity/desk-tool": "^1.149.16",
    "@sanity/vision": "^1.149.16",
    "nav-frontend-js-utils": "^1.0.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@testing-library/react": "^11.2.2",
    "@types/styled-components": "^5.1.0",
    "babel-jest": "^26.6.3",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-test-renderer": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,less,css,md}": [
      "prettier --write"
    ]
  }
}
